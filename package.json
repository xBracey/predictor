{
  "name": "euro-predictor",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "server-dev": "nodemon --watch src --exec babel-node src/index.js",
    "server": "babel-node src/index.js",
    "build": "next build; next export",
    "start": "yarn web-scss; yarn build; yarn server",
    "web-dev": "concurrently --kill-others \"yarn scss\" \"next dev\"",
    "scss": "node-sass --watch ./styles/main.scss -o ./styles",
    "web-scss": "node-sass ./styles/main.scss -o ./styles",
    "dev": "concurrently --kill-others \"yarn scss\" \"nodemon --exec 'yarn build'\" \"yarn server\""
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@zeit/next-css": "^1.0.1",
    "bcrypt": "^3.0.6",
    "concurrently": "^4.1.2",
    "connect-ensure-login": "^0.1.1",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "jsonwebtoken": "^8.5.1",
    "next": "9.0.5",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "sequelize": "^5.12.3",
    "uuid": "^3.3.3"
  },
  "nodemonConfig": {
    "watch": [
      "pages",
      "components",
      "styles/main.css"
    ]
  }
}
