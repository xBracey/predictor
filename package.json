{
  "name": "euro-predictor",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "pm2 start --interpreter babel-node src/index.js",
    "build-pages": "next build; next export",
    "pre-start": "rm -rf out; rm -rf .next; yarn build-pages; yarn scss;",
    "start": "yarn pre-start; yarn build",
    "scss": "node-sass ./styles/main.scss -o ./out/static",
    "dev-scss": "node-sass --watch ./styles/main.scss -o ./out/static",
    "dev": "rm -rf out; rm -rf .next; yarn build-pages; yarn scss; concurrently --kill-others \"yarn dev-scss\" \"nodemon --watch src --exec babel-node src/index.js\""
  },
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@zeit/next-css": "^1.0.1",
    "bcrypt": "^3.0.6",
    "concurrently": "^4.1.2",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^2.8.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "jsonwebtoken": "^8.5.1",
    "jwt-simple": "^0.5.6",
    "moment": "^2.24.0",
    "mysql2": "^1.5.2",
    "next": "9.3.2",
    "node-sass": "^4.12.0",
    "nodemailer": "^6.4.16",
    "nodemon": "^1.19.1",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "pg": "^7.12.0",
    "pm2": "^4.2.3",
    "react": "16.9.0",
    "react-datepicker": "^2.10.1",
    "react-dom": "16.9.0",
    "sequelize": "^5.15.1",
    "uuid": "^3.3.3"
  },
  "nodemonConfig": {
    "watch": [
      "pages",
      "components",
      "styles/main.css"
    ]
  }
}
